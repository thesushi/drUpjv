<div id="jssor_1"
     style="position:relative;margin:0 auto;top:0px;left:0px;width:980px;height:300px;overflow:hidden;visibility:hidden;">
    <!-- Loading Screen -->
    <div data-u="loading" style="position: absolute; top: 0px; left: 0px;">
        <div style="filter: alpha(opacity=70); opacity: 0.7; position: absolute; display: block; top: 0px; left: 0px; width: 100%; height: 100%;">
            &nbsp;</div>
        <div style="position:absolute;display:block;background:url('themes/upjv/img/loading.gif') no-repeat center center;top:0px;left:0px;width:100%;height:100%;">
            &nbsp;</div>
    </div>

    <div data-u="slides"
         style="cursor: default; position: relative; top: 0px; left: 0px; width: 980px; height: 300px; overflow: hidden;">

        {% for row in rows %}

            {{ row.content }}

        {% endfor %}


    </div>
    <!-- Arrow Navigator -->
    <span data-u="arrowleft" class="jssora07l" style="top:0px;left:8px;width:50px;height:50px;" data-autocenter="2">&nbsp;</span>
    <span data-u="arrowright" class="jssora07r" style="top:0px;right:8px;width:50px;height:50px;" data-autocenter="2">&nbsp;</span>
</div>
